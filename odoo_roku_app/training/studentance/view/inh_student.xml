<?xml version="1.0" encoding="utf-8"?>

<odoo>
	<record model="ir.ui.view" id="student_id11">
            <field name="name">student.inherit.tree.view</field>
            <field name="model">student.inherit</field>
            <field name="arch" type="xml">

            	<tree>
            		<field name="name"/>
            		<field name="passout"/>
            		<field name="job"/>
            	</tree>
            </field>
        </record>


        <record model="ir.ui.view" id="student_form_inhrit">
            <field name="name">student.inherit.form.view</field>
            <field name="model">student.inherit</field>
            <field name="arch" type="xml">

                <form>
                    <notebook>
                        <page>
                            <group>
                        <field name="name"/>
                        <field name="passout"/>
                        <field name="job"/>
                            </group>
                    </page>
                    </notebook>
                </form>
            </field>
        </record>


 	<record id="student_action" model="ir.actions.act_window">
        <field name="name">student Inherit</field>
        <field name="res_model">student.inherit</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="student_menu_root" name="Student inherit"/>
		<menuitem id="student__menu_1" parent="student_menu_root" sequence="1" name="studentINH" action="student_action"/>
</odoo>