<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<!-- Tree view student -->
	<record model="ir.ui.view" id="tree_view_employee">
		<field name = "name">Employee</field>
		<field name = "model">employee</field>
		<field name="arch" type="xml">
			<tree>
				<field name = "first_name"/>
				<field name = "middle_name"/>
				<field name = "last_name"/>
			</tree>
		</field>
	</record>
	  <!-- Action for Employee -->

    <record id="employee_action" model="ir.actions.act_window">
        <field name="name">Employees</field>
        <field name="res_model">employee</field>
        <field name="view_mode">tree</field>
    </record>
    <!-- Menu for student -->
    <menuitem id="employee_menu" name="Employee Data"/>  <!-- This name is shown as application name-->
 
    <menuitem id="menu_employee_child1" name="Attendance" action="employee_action" parent="employee_menu"/>
    <menuitem id="menu_employee_child2" name="Time Off" action="employee_action" parent="employee_menu"/>
</odoo>