<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record model="ir.ui.view" id="student_view_tree">
    	<field name="name">Student.tree</field>
   		<field name="model">student</field>
    	<field name="arch" type="xml">
        	<tree string="Student list">
    			<field name="name"/>
    			<field name="branch"/>
			</tree>
    	</field>
	</record>

	<record model="ir.actions.act_window" id="student_action">
    <field name="name">Student.action</field>
    <field name="res_model">student</field>
    <field name="view_mode">tree</field>
</record>
<menuitem id="student_data"  name="Student Data" />
<menuitem id="students" parent="student_data" name="Details" action="student_action"/>
</odoo>