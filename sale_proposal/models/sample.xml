This XML file does not appear to have any style information associated with it. The document tree is shown below.
<odoo>
    <data>
        <template id="proposal_portal_template" name="Proposal Portal Template" inherit_id="portal.portal_sidebar"
                  primary="True">
            <xpath expr="//div[hasclass('o_portal_sidebar')]" position="inside">
                <t t-set="o_portal_fullwidth_alert">
                    <t t-call="portal.portal_back_in_edit_mode">
                        <t t-set="backend_url"
                           t-value="'/web#model=%s&id=%s&action=%s&view_type=form' % (proposals_proposals._name, proposals_proposals.id, action.id)"/>
                    </t>
                </t>
                <div class="row mt16 o_portal_proposal_sidebar">
                    <!--  Sidebar  -->
                    <t t-call="portal.portal_record_sidebar">
                        <t t-set="classes" t-value="'col-lg-auto d-print-none'"/>
                        <t t-set="title">
                            <h2 class="mb-0">
                                <b t-field="proposals_proposals.accepted_total_price" data-id="accepted_total_price"/>
                            </h2>
                        </t>
                        <t t-set="entries">
                            <ul class="list-group list-group-flush flex-wrap flex-row flex-lg-column">
                                <li class="list-group-item flex-grow-1">
                                    <div class="o_download_pdf btn-toolbar flex-sm-nowrap">
                                        <div class="btn-group flex-grow-1 mr-1 mb-1">
                                            <a class="btn btn-secondary btn-block o_download_btn"
                                               t-att-href="proposals_proposals.get_portal_url(report_type='pdf', download=True)"
                                               title="Download">
                                                <i class="fa fa-download"/>
                                                Download
                                            </a>
                                        </div>
                                        <div class="btn-group flex-grow-1 mb-1">
                                            <a class="btn btn-secondary btn-block o_print_btn o_portal_invoice_print"
                                               t-att-href="proposals_proposals.get_portal_url(report_type='pdf')"
                                               id="print_invoice_report" title="Print" target="_blank">
                                                <i class="fa fa-print"/>
                                                Print
                                            </a>
                                        </div>
                                    </div>
                                </li>
                                <li class="navspy list-group-item pl-0 flex-grow-1" t-ignore="true"
                                    role="complementary">
                                    <ul class="nav flex-column bs-sidenav"/>
                                </li>
                                <li t-if="proposals_proposals.sales_man_id" class="list-group-item flex-grow-1">
                                    <div class="small mb-1">
                                        <strong class="text-muted">Salesperson</strong>
                                    </div>
                                    <div class="row flex-nowrap">
                                        <div class="col flex-grow-0 pr-2">
                                            <img class="rounded-circle mr4 float-left o_portal_contact_img"
                                                 t-if="proposals_proposals.sales_man_id.image_1024"
                                                 t-att-src="image_data_uri(proposals_proposals.sales_man_id.image_1024)"
                                                 alt="Contact"/>
                                            <img class="rounded-circle mr4 float-left o_portal_contact_img"
                                                 t-if="not proposals_proposals.sales_man_id.image_1024"
                                                 src="/web/static/src/img/placeholder.png" alt="Contact"/>
                                        </div>
                                        <div class="col pl-0" style="min-width: 150px">
                                            <span t-field="proposals_proposals.sales_man_id" t-options="{" widget": "
                                            contact", "fields": ["name", "phone"], "no_marker": True}"/>
                                            <a href="#discussion" class="small">
                                                <i class="fa fa-comment"/>
                                                Send message
                                            </a>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </t>
                    </t>
                    <!--  Page content  -->
                    <div id="proposal_content" class="col-12 col-lg justify-content-end">
                        <!--  main content  -->
                        <div t-attf-class="card #{'pb-5' if report_type == 'html' else ''}">
                            <div t-call="proposal_week_2.proposal_portal_content"/>
                        </div>
                        <!--  bottom actions  -->
                        <div t-attf-class="row justify-content-center text-center d-print-none pt-1 pb-4">
                            <div t-if="proposals_proposals.proposal_status != 'accept' and proposals_proposals.proposal_status != 'reject'"
                                 class="col-sm-auto mt8">
                                <a role="button" class="btn btn-primary" data-toggle="modal" data-target="#modalaccept"
                                   t-att-href="proposals_proposals.get_portal_url(suffix='/accept')">
                                    <i class="fa fa-check"/>
                                    <t>Accept</t>
                                </a>
                            </div>
                            <div t-if="proposals_proposals.proposal_status != 'accept' and proposals_proposals.proposal_status != 'reject'"
                                 class="col-sm-auto mt8">
                                <a role="button" class="btn btn-danger" data-toggle="modal" data-target="#modaldecline"
                                   t-att-href="proposals_proposals.get_portal_url(suffix='/reject')">
                                    <i class="fa fa-times"/>
                                    Reject
                                </a>
                            </div>
                        </div>
                        <!--  modal relative to the action reject  -->
                        <div role="dialog" class="modal fade" id="modaldecline">
                            <div class="modal-dialog">
                                <form id="decline" method="POST"
                                      t-attf-action="/my/proposals/#{proposals_proposals.id}/decline?access_token=#{proposals_proposals.access_token}"
                                      class="modal-content">
                                    <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                                    <header class="modal-header">
                                        <h4 class="modal-title">Reject This Proposal</h4>
                                        <button type="button" class="close" data-dismiss="modal"
                                                aria-label="Close">&times;
                                        </button>
                                    </header>
                                    <main class="modal-body">
                                        <p>Tell us why you are refusing this proposal, this will help us improve our
                                            services.
                                        </p>
                                        <textarea rows="4" name="decline_message" required=""
                                                  placeholder="Your feedback..." class="form-control"/>
                                    </main>
                                    <footer class="modal-footer">
                                        <button type="submit" t-att-id="proposals_proposals.id" class="btn btn-danger">
                                            <i class="fa fa-times"
                                               t-att-href="proposals_proposals.get_portal_url(suffix='/decline')"/>
                                            Reject
                                        </button>
                                        <button type="button" class="btn btn-primary" data-dismiss="modal">Cancel
                                        </button>
                                    </footer>
                                </form>
                            </div>
                        </div>
                        <div role="dialog" class="modal fade" id="modalaccept">
                            <div class="modal-dialog">
                                <form id="accepting" method="POST"
                                      t-attf-action="/my/proposals/#{proposals_proposals.id}/accept?access_token=#{proposals_proposals.access_token}"
                                      class="modal-content">
                                    <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                                    <header class="modal-header">
                                        <h4 class="modal-title">Accept This Proposal</h4>
                                        <button type="button" class="close" data-dismiss="modal"
                                                aria-label="Close">&times;
                                        </button>
                                    </header>
                                    <main class="modal-body">
                                        <p>Are you sure you want to accept this proposal?</p>
                                        <textarea rows="4" name="accept_message" required=""
                                                  placeholder="Your feedback..." class="form-control"/>
                                    </main>
                                    <footer class="modal-footer">
                                        <button type="submit" t-att-id="proposals_proposals.id" class="btn btn-primary">
                                            <i class="fa fa-check"
                                               t-att-href="proposals_proposals.get_portal_url(suffix='/accept')"/>
                                            Accept
                                        </button>
                                        <button type="button" class="btn btn-primary" data-dismiss="modal">Cancel
                                        </button>
                                    </footer>
                                </form>
                            </div>
                        </div>
                        <!--  chatter  -->
                        <div id="proposal_communication" class="mt-4">
                            <h2>History</h2>
                            <t t-call="portal.message_thread">
                                <t t-set="object" t-value="proposals_proposals"/>
                            </t>
                        </div>
                    </div>
                </div>
            </xpath>
        </template>
        <template id="proposal_portal_content_totals_table">
            <table class="table table-sm">
                <tr class="border-black">
                    <td>
                        <strong>Proposaed Total</strong>
                    </td>
                    <td class="text-right">
                        <span data-id="total_untaxed" t-field="proposals_proposals.proposed_total_price" t-options="{"
                              widget": "monetary","display_currency": proposals_proposals.price_list_id.currency_id}"/>
                    </td>
                </tr>
                <tr class="border-black">
                    <td>
                        <strong>Accepted Total</strong>
                    </td>
                    <td class="text-right">
                        <span data-id="total_amount" t-field="proposals_proposals.accepted_total_price" t-options="{"
                              widget": "monetary", "display_currency": proposals_proposals.price_list_id.currency_id}"/>
                    </td>
                </tr>
            </table>
        </template>
        <template id="proposal_portal_content" name="Proposal Portal Content">
            <!--  Intro  -->
            <div id="introduction" t-attf-class="pb-2 pt-3 #{'card-header bg-white' if report_type == 'html' else ''}">
                <h2 class="my-0">
                    <em t-esc="proposals_proposals.proposal_name"/>
                </h2>
            </div>
            <div t-attf-class="#{'card-body' if report_type == 'html' else ''}">
                <!--  Informations  -->
                <div id="informations">
                    <div class="row" id="so_date">
                        <div class="mb-3 col-6">
                            <strong>Proposal Date:</strong>
                            <span t-field="proposals_proposals.proposal_date" t-options="{" widget": "date"}"/>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-6">
                            <strong class="d-block mb-1">Customer:</strong>
                            <address t-field="proposals_proposals.customer_id" t-options="{" widget": "contact", "fields
                            ": ["address", "name"], "no_marker": True}"/>
                        </div>
                    </div>
                </div>
                <section id="details" style="page-break-inside: auto;" class="mt32">
                    <h3 id="details">Pricing</h3>
                    <t t-foreach="proposals_proposals.proposal_line_ids" t-as="line"/>
                    <table class="table table-sm" id="proposal_table">
                        <thead class="bg-100">
                            <tr>
                                <th class="text-left">Products Image</th>
                                <th class="text-left">Products</th>
                                <th class="text-right">Proposed Quantity</th>
                                <th t-attf-class="text-right {{ 'd-none d-sm-table-cell' if report_type == 'html' else '' }}">
                                    Proposed Unit Price
                                </th>
                                <th class="text-right">Accepted Quantity</th>
                                <th t-attf-class="text-right {{ 'd-none d-sm-table-cell' if report_type == 'html' else '' }}">
                                    Accepted Unit Price
                                </th>
                            </tr>
                        </thead>
                        <tbody class="sale_tbody">
                            <t t-foreach="proposals_proposals.proposal_line_ids" t-as="line">
                                <tr t-att-class="'bg-200 font-weight-bold o_line_section'">
                                    <td>
                                        <img t-att-src="image_data_uri(line.product_id.image_1024)"
                                             style="max-height: 40px; max-width: 80px;"/>
                                    </td>
                                    <td id="product_name">
                                        <span t-field="line.product_id"/>
                                    </td>
                                    <td class="text-right">
                                        <div id="proposed_qty">
                                            <span t-field="line.qty_proposed"/>
                                        </div>
                                    </td>
                                    <td t-attf-class="text-right">
                                        <div id="proposed_price">
                                            <span t-field="line.price_proposed"/>
                                        </div>
                                    </td>
                                    <td class="text-right">
                                        <div id="qty_accepted">
                                            <span t-field="line.qty_accepted"/>
                                        </div>
                                    </td>
                                    <td t-attf-class="text-right">
                                        <div id="accepted_price">
                                            <span t-field="line.price_accepted"/>
                                        </div>
                                    </td>
                                </tr>
                            </t>
                        </tbody>
                    </table>
                    <div id="total" class="row" name="total" style="page-break-inside: avoid;">
                        <div t-attf-class="#{'col-4' if report_type != 'html' else 'col-sm-7 col-md-5'} ml-auto">
                            <table class="table table-sm">
                                <tr class="border-black ">
                                    <td class="text-right">
                                        <strong>Proposed Total</strong>
                                    </td>
                                    <td class="text-right">
                                        <span data-id="total_amount_1"
                                              t-field="proposals_proposals.proposed_total_price" t-options="{" widget":
                                        "monetary", "display_currency": proposals_proposals.price_list_id.currency_id}"
                                        />
                                    </td>
                                </tr>
                                <tr class="border-black">
                                    <td class="text-right">
                                        <strong>Accepted Total</strong>
                                    </td>
                                    <td class="text-right">
                                        <span data-id="total_amount_2"
                                              t-field="proposals_proposals.accepted_total_price" t-options="{" widget":
                                        "monetary", "display_currency": proposals_proposals.price_list_id.currency_id}"
                                        />
                                    </td>
                                </tr>
                            </table>
                        </div>
                    </div>
                </section>
            </div>
        </template>
    </data>
</odoo>